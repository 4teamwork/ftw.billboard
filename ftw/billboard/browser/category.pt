<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      lang="en"
      metal:use-macro="here/main_template/macros/master"
      i18n:domain="ftw.billboard">

<body>

<div metal:fill-slot="main">
    <metal:main-macro define-macro="main">
    <div tal:replace="structure provider:plone.abovecontenttitle" />
    <h1 tal:content="here/Title" class="documentFirstHeading" />
    <div tal:replace="structure provider:plone.belowcontenttitle" />
    <div tal:replace="structure provider:plone.abovecontentbody" />

    <tal:elements condition="view/get_elements">
        <table class="listing">
            <tr tal:repeat="element view/get_elements">
                <td>
                    xx.xx.xxxx
                </td>
                <td>
                    <span tal:content="structure element/Title" />
                </td>
            </tr>
        </table>
    </tal:elements>
    <span tal:condition="not: view/get_elements"
          i18n:translate="">
        no ads
    </span>
    </metal:main-macro>
</div>
</body>
</html>
